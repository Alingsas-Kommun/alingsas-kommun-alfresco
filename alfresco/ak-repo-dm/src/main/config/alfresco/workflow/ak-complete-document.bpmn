          <omgdc:Bounds height="13.0" width="100.0" x="-65.0" y="-19.0"></omgdc:Bounds>
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="approve_publishing_flow" id="BPMNEdge_approve_publishing_flow">
