<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.alingsas.se/alfresc/workflow">
  <process id="completeDocument" name="Complete document process definition" isExecutable="true">
    <extensionElements>
      <activiti:executionListener event="start" class="org.alfresco.repo.workflow.activiti.listener.ScriptExecutionListener">
        <activiti:field name="script">
          <activiti:string>logger.log("Starting Complete document WF");

if (akwf_targetFolder.hasPermission("Write")) {
	execution.setVariable('akwf_handling', 'Automatic');
	logger.log("User have write permission on target folder, setting WF handling to Automatic");
} else {
	execution.setVariable('akwf_handling', 'Manual');
	logger.log("User have read permission on target folder, setting WF handling to Manual");
}
          
logger.log("Verify that user have write access to all attached files");     
if (bpm_package != null &amp;&amp; bpm_package.children != null) {
    var i = 0;
    for (i = 0; i &lt; bpm_package.children.length; i++) {
		var document = bpm_package.children[i];
		var hasPermission = document.hasPermission("Write");
		if (hasPermission) {
			logger.log("User has permission to complete file "+document.name);
		} else {
			logger.log("User has not permission to complete file "+document.name);
			logger.log("Setting WF handling to Error");
			execution.setVariable('akwf_handling', 'Error');
		}
		
    }
}
          				
var node = akwf_targetFolder;
var site = node.getSiteShortName();
logger.log("Target folder belongs to site "+site);
//Find collaborator group
var group = groups.getGroup('site_'+site+'_SiteCollaborator');
var groupMembers = group.getAllUsers();
//If group was not found or no users were in group, try the Manager group instead. There should be at least one
if (group==null || groupMembers.length==0) {
	group = groups.getGroup('site_'+site+'_SiteManager')
}      
var groupName = group.getFullName();
logger.log("Group for assigning review task"+groupName);
execution.setVariable('akwf_site', site);
execution.setVariable('akwf_siteGroup', groupName);

execution.setVariable('akwf_initiator', person.properties["cm:userName"]);</activiti:string>
        </activiti:field>
      </activiti:executionListener>
    </extensionElements>
    <startEvent id="start" name="Start" activiti:formKey="akwf:completeDocument"></startEvent>
    <sequenceFlow id="start_flow" sourceRef="start" targetRef="autoManDecision"></sequenceFlow>
    <exclusiveGateway id="autoManDecision" name="Decides if automatic or manual handling should be done for this process"></exclusiveGateway>
    <sequenceFlow id="manual_publishing_flow" name="Manuell hantering" sourceRef="autoManDecision" targetRef="reviewDocumentTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${akwf_handling== 'Manual'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="reviewDocumentTask" name="Dokumentansvarig på site skall godkänna färdigställande av dokument" activiti:candidateGroups="${akwf_siteGroup}" activiti:formKey="akwf:completeDocumentReviewDocumentTask">
      <extensionElements>
        <activiti:taskListener event="create" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>if (typeof bpm_workflowDueDate != 'undefined') task.dueDate = bpm_workflowDueDate;
if (typeof bpm_workflowPriority != 'undefined') task.priority = bpm_workflowPriority;
if (typeof bpm_comment != 'undefined') task.setVariable('bpm_comment', bpm_comment);</activiti:string>
          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>logger.log("Review of document completion complete");	
						execution.setVariable('bpm_comment', task.getVariable('bpm_comment'));
execution.setVariable('bpm_dueDate', task.dueDate);
execution.setVariable('bpm_priority', task.priority);
execution.setVariable('akwf_reviewOutcome', task.getVariable('akwf_reviewOutcome'));
execution.setVariable('akwf_targetFolder', task.getVariable('akwf_targetFolder'));
logger.log("Outcome set to "+task.getVariable('akwf_reviewOutcome'));</activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="review_decision_flow" sourceRef="reviewDocumentTask" targetRef="reviewDecision"></sequenceFlow>
    <exclusiveGateway id="reviewDecision" name="Review Decision"></exclusiveGateway>
    <sequenceFlow id="approve_publishing_flow" name="Godkän publicering" sourceRef="reviewDecision" targetRef="completeDocumentScript">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${akwf_reviewOutcome== 'Approve'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="auto_publishing_flow" name="Automatic publishing" sourceRef="autoManDecision" targetRef="completeDocumentScript">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${akwf_handling== 'Automatic'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="completeDocumentScript" name="Färdigställ dokument" activiti:class="org.alfresco.repo.workflow.activiti.script.AlfrescoScriptDelegate">
      <extensionElements>
        <activiti:field name="runAs">
          <activiti:string>admin</activiti:string>
        </activiti:field>
        <activiti:field name="script">
          <activiti:string>logger.log("Completing document");	

if (bpm_package != null &amp;&amp; bpm_package.children != null) {
    var i = 0;
    for (i = 0; i &lt; bpm_package.children.length; i++) {
		var document = bpm_package.children[i];
		logger.log("Moving document");
		document.move(akwf_targetFolder);
		document.properties["akdm:documentStatus"] = "Färdigt dokument";
		logger.log("Setting/saving properties");
		document.save();
		//Disable set owner, the original owner will remain, uncomment to set owner to the one who started the flow
		//var initiator = execution.getVariable("akwf_initiator");
		//document.setOwner(initiator);
    }
}</activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="deny_publishing_flow" name="Neka publicering" sourceRef="reviewDecision" targetRef="userReviewResultReceiptTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${akwf_reviewOutcome== 'Reject'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="user_receipt_review_flow" sourceRef="completeDocumentScript" targetRef="userReviewResultReceiptTask"></sequenceFlow>
    <sequenceFlow id="error_flow" name="Felhantering" sourceRef="autoManDecision" targetRef="userReviewResultReceiptTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${akwf_handling== 'Error'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="userReviewResultReceiptTask" name="Kvittens på färdigställande" activiti:assignee="${akwf_initiator}" activiti:formKey="akwf:completeDocumentUserReviewResultReceiptTask"></userTask>
    <sequenceFlow id="end_flow" sourceRef="userReviewResultReceiptTask" targetRef="end"></sequenceFlow>
    <endEvent id="end" name="End"></endEvent>
  </process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_completeDocument">
