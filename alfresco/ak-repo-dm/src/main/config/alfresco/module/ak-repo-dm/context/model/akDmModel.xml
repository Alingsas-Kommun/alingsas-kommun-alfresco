<?xml version="1.0" encoding="UTF-8"?>

<model name="akdm:documentmanagement" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>Alingsås Kommun Document Management</description>
	<author>Repill Linpro AB</author>
	<version>1.0.0</version>

	<imports>
		<!-- Import Alfresco System Definitions -->
		<import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
		<!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0"
			prefix="d" />
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<!-- Import Alfresco Rendition Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/rendition/1.0"
			prefix="rn" />
	</imports>

	<namespaces>
		<namespace uri="http://www.alingsas.se/alfresco/model/dm/1.0"
			prefix="akdm" />
	</namespaces>
	<constraints>
		<constraint name="akdm:yesNoConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value></value>
					<value>Ja</value>
					<value>Nej</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
		<constraint name="akdm:groupConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value></value>
					<value>Föreskrifter</value>
					<value>Policy</value>
					<value>Reglementen</value>
					<value>Regler</value>
					<value>Rutiner</value>
					<value>Taxor/Avgifter</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
		<constraint name="akdm:functionConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value></value>
					<value>Personal</value>
					<value>Ekonomi</value>
					<value>Administration</value>
					<value>Verksamhet</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
		<constraint name="akdm:manualConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value></value>
					<value>PA</value>
					<value>Lön</value>
					<value>Arbetsmiljö</value>
					<value>Ekonomi</value>
					<value>Administration</value>
					<value>Verksamhet</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
		<constraint name="akdm:docStatusConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>Arbetsdokument</value>
					<value>Färdigt dokument</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
		<constraint name="akdm:secrecyStatusConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>Sekretessbelagt</value>
					<value>Offentligt</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
	</constraints>
	<types>
		<type name="akdm:document">
			<title>Alingsås Kommun Dokument</title>
			<parent>cm:content</parent>
			<mandatory-aspects>
				<aspect>akdm:commonAspect</aspect>
				<aspect>cm:versionable</aspect>
			</mandatory-aspects>
		</type>
		<type name="akdm:instruction">
			<title>Anvisning</title>
			<parent>akdm:document</parent>
			<mandatory-aspects>
				<aspect>akdm:protocolAspect</aspect>
				<aspect>akdm:instructionAspect</aspect>
			</mandatory-aspects>
		</type>
		<type name="akdm:ordinance">
			<title>Författning</title>
			<parent>akdm:document</parent>
			<mandatory-aspects>
				<aspect>akdm:protocolAspect</aspect>
				<aspect>akdm:instructionAspect</aspect>
			</mandatory-aspects>
		</type>
		<type name="akdm:manual">
			<title>Handbok</title>
			<parent>akdm:document</parent>
			<mandatory-aspects>
				<aspect>akdm:manualAspect</aspect>
			</mandatory-aspects>
		</type>
		<type name="akdm:protocol">
			<title>Protokoll</title>
			<parent>akdm:document</parent>
			<mandatory-aspects>
				<aspect>akdm:protocolAspect</aspect>
			</mandatory-aspects>
		</type>
		<type name="akdm:generaldocument">
			<title>Generellt dokument</title>
			<parent>akdm:document</parent>
			<mandatory-aspects>
				<aspect>akdm:generalDocumentAspect</aspect>
			</mandatory-aspects>
		</type>
		<type name="akdm:economydocument">
			<title>Ekonomidokument</title>
			<parent>akdm:document</parent>
		</type>
		<!-- <type name="akdm:byggreda"> <title>ByggReda</title> <parent>akdm:document</parent> 
			<mandatory-aspects> <aspect>akdm:byggRedaAspect</aspect> </mandatory-aspects> 
			</type> -->
		<type name="akdm:folder">
			<title>Alingsås Kommun Folder</title>
			<parent>cm:folder</parent>
		</type>
	</types>

	<aspects>
		<aspect name="akdm:aspect">
			<title>Alingsås Kommun Aspect</title>
		</aspect>
		<aspect name="akdm:commonAspect">
			<title>Generella metadata</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:documentNumber">
					<title>Dokumentnummer</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:documentStatus">
					<title>Dokumentstatus</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<default>Arbetsdokument</default>
					<constraints>
						<constraint ref="akdm:docStatusConstraint" />
					</constraints>
				</property>
				<property name="akdm:documentSecrecy">
					<title>Sekretess</title>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<constraints>
						<constraint ref="akdm:secrecyStatusConstraint" />
					</constraints>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:instructionAspect">
			<title>Anvisning Aspect</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:instructionDecisionDate">
					<title>Beslutsdatum</title>
					<type>d:date</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:instructionGroup">
					<title>Grupp</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
						<constraint ref="akdm:groupConstraint" />
					</constraints>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:manualAspect">
			<title>Manual Aspect</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:manualFunction">
					<title>Funktion</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
						<constraint ref="akdm:functionConstraint" />
					</constraints>
				</property>
				<property name="akdm:manualManual">
					<title>Handbok</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
						<constraint ref="akdm:manualConstraint" />
					</constraints>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:protocolAspect">
			<title>Protokoll Aspect</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:protocolId">
					<title>Protokollid</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:generalDocumentAspect">
			<title>Generellt dokument Aspect</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:generalDocumentDescription">
					<title>Beskrivning</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:generalDocumentDate">
					<title>Datum</title>
					<type>d:date</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</aspect>
