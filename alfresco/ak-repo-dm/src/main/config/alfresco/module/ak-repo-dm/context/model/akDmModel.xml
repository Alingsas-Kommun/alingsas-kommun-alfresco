<?xml version="1.0" encoding="UTF-8"?>

<model name="akdm:documentmanagement" xmlns="http://www.alfresco.org/model/dictionary/1.0">

	<description>Alingsås Kommun Document Management</description>
	<author>Repill Linpro AB</author>
	<version>1.0.0</version>

	<imports>
		<!-- Import Alfresco System Definitions -->
		<import uri="http://www.alfresco.org/model/system/1.0" prefix="sys" />
		<!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0"
			prefix="d" />
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
		<!-- Import Alfresco Rendition Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/rendition/1.0"
			prefix="rn" />
	</imports>

	<namespaces>
		<namespace uri="http://www.alingsas.se/alfresco/model/dm/1.0"
			prefix="akdm" />
	</namespaces>
	<constraints>
		<constraint name="akdm:jaNejConstraint"
			type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>Ja</value>
					<value>Nej</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
		<constraint name="akdm:groupConstraint"
			type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>Föreskrifter</value>
					<value>Policy</value>
					<value>Reglementen</value>
					<value>Regler</value>
					<value>Rutiner</value>
					<value>Taxor/Avgifter</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
		<constraint name="akdm:functionConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>Personal</value>
					<value>Ekonomi</value>
					<value>Administration</value>
					<value>Verksamhet</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
		<constraint name="akdm:handbookConstraint" type="LIST">
			<parameter name="allowedValues">
				<list>
					<value>PA</value>
					<value>Lön</value>
					<value>Arbetsmiljö</value>
					<value>Ekonomi</value>
					<value>Administration</value>
					<value>Verksamhet</value>
				</list>
			</parameter>
			<parameter name="caseSensitive">
				<value>false</value>
			</parameter>
		</constraint>
	</constraints>
	<types>
		<type name="akdm:document">
			<title>Alingsås Kommun Document</title>
			<parent>cm:content</parent>
		</type>
		<type name="akdm:folder">
			<title>Alingsås Kommun Folder</title>
			<parent>cm:folder</parent>
		</type>
	</types>

	<aspects>
		<aspect name="akdm:aspect">
			<title>Alingsås Kommun Aspect</title>
		</aspect>
		<aspect name="akdm:directionAspect">
			<title>Anvisning</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:directionProtocolId">
					<title>Protokollid</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:directionDecisionDate">
					<title>Beslutsdatum</title>
					<type>d:date</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:directionGroup"><constraints>
						<constraint ref="akdm:handbookConstraint" />
					</constraints>
					<title>Grupp</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
						<constraint ref="akdm:groupConstraint" />
					</constraints>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:handbookAspect">
			<title>Handbok</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:handbookFunction">
					<title>Funktion</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
						<constraint ref="akdm:functionConstraint" />
					</constraints>
				</property>
				<property name="akdm:handbookHandbook">
					<title>Handbok</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
						<constraint ref="akdm:handbookConstraint" />
					</constraints>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:protocolAspect">
			<title>Protokoll</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:protocolProtocolId">
					<title>Protokollid</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:frameworkAgreementAspect">
			<title>Ramavtal</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:frameworkAgreementReferenceNumber">
					<title>Referensnr/Diarienr</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:frameworkAgreementDatePeriod">
					<title>Avtalstid</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:generalDocumentAspect">
			<title>Generellt dokument</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:generalDocumentDescription">
					<title>Beskrivning</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:generalDocumentDate">
					<title>Datum</title>
					<type>d:date</type>
					<mandatory>false</mandatory>
				</property>
			</properties>
		</aspect>
		<aspect name="akdm:byggRedaAspect">
			<title>ByggReda</title>
			<parent>akdm:aspect</parent>
			<properties>
				<property name="akdm:byggRedaFilm">
					<title>Film</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaLopnr">
					<title>Löpnummer</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaDnr">
					<title>Diarienummer</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaFastBet">
					<title>Fastighetsbeteckning</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaTidFastBet">
					<title>Tidigare fastighetsbeteckning</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaAdress">
					<title>Adress</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaTidAdress">
					<title>Tidigare adress</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaBeslut">
					<title>Beslut</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaAvser">
					<title>Beslut</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaArendemening">
					<title>Ärendemening</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaAnteckning">
					<title>Anteckning</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaHandlingar">
					<title>Handlingar</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
				</property>
				<property name="akdm:byggRedaSekretess">
					<title>Sekretess</title>
					<type>d:text</type>
					<mandatory>false</mandatory>
					<constraints>
						<constraint ref="akdm:jaNejConstraint" />
					</constraints>
				</property>
			</properties>
		</aspect>
		
	</aspects>

</model>
