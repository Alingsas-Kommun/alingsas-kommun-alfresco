<?xml version='1.0' encoding='UTF-8'?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Bean configurations for DD Migration script -->
	<bean
		id="webscript.se.alingsas.alfresco.repo.scripts.migration.ddmigration.get"
		class="se.alingsas.alfresco.repo.scripts.Migrate" parent="webscript">
		<property name="baseFolder" value="/opt/alfresco/ddmigration" />
	</bean>
	
	<!-- Bean configurations for User Homes Linking script -->
	<bean
		id="webscript.se.alingsas.alfresco.repo.scripts.userhome.link.get"
		class="se.alingsas.alfresco.repo.scripts.LinkUserHome" parent="webscript">
	</bean>
	
	<!-- Bean configurations for ByggRedaImpor script -->
	<bean
		id="webscript.se.alingsas.alfresco.repo.scripts.byggreda.import.get"
		class="se.alingsas.alfresco.repo.scripts.ByggRedaImport" parent="webscript">
	</bean>
</beans>
