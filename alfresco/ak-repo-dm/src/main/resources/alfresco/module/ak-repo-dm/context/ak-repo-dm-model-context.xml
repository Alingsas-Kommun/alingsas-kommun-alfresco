<?xml version='1.0' encoding='UTF-8'?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <!-- Registration of new model -->
  <bean id="ak.dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="dictionaryBootstrap">
    <property name="models">
      <list>
        <value>alfresco/module/ak-repo-dm/model/akDmModel.xml</value>
      </list>
    </property>
    <property name="labels">
      <list>
        <value>alfresco/module/ak-repo-dm/messages/ak-dm-model</value>
      </list>
    </property>
    
  </bean>
</beans>
