<?xml version='1.0' encoding='UTF-8'?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


 <bean id="ak-repo-dm-patch-messages" class="org.alfresco.i18n.ResourceBundleBootstrapComponent">
    <property name="resourceBundles">
      <list>
        <value>alfresco.module.ak-repo-dm.context.messages.ak-patch-messages</value>
      </list>
    </property>
  </bean>

  <bean id="ak-patch.cancelRunningWorkflowReceipts" class="se.alingsas.alfresco.repo.admin.patch.CloseRunningWorkflowreceiptsPatch" parent="basePatch" depends-on="ak.workflowBootstrap">
    <property name="id">
      <value>patch.cancelRunningWorkflowReceipts</value>
    </property>
    <property name="description">
      <value>patch.cancelRunningWorkflowReceipts.description</value>
    </property>
    <property name="fixesFromSchema">
      <value>0</value>
    </property>
    <property name="fixesToSchema">
      <value>${version.schema}</value>
    </property>
    <property name="targetSchema">
      <value>10000</value>
    </property>
    <property name="dependsOn">
      <list>
        <ref bean="patch.updateDmPermissions" />
      </list>
    </property>
    <property name="workflowService">
      <ref bean="WorkflowService" />
    </property>
  </bean>
 
</beans>
    